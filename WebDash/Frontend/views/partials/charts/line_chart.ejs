<div class="my-3">
  <canvas id="line_chart"></canvas>
</div>

<script>
  var ctx = document.getElementById("line_chart").getContext("2d");
  var lab = "<%= labels %>".split(",");
  var chartData = "<%= chartData %>".split(",").map(Number);

  var myChart = new Chart(ctx, {
    type: "line",
    data: {
      labels: lab,
      datasets: [
        {
          label: "Dataset",
          data: chartData,
          fill: false,
          borderColor: "rgb(75, 192, 192)",
          tension: 0.1,
        },
      ],
    },
  });
</script>
